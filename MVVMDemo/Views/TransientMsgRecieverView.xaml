<?xml version="1.0" encoding="utf-8" ?>
<viewbase:ViewBase xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewbase="clr-namespace:MVVMDemo.Views.Base"
             x:Class="MVVMDemo.Views.TransientMsgRecieverView"
             xmlns:viewmodels="clr-namespace:MVVMDemo.ViewModels"

             x:TypeArguments="viewmodels:TransientMsgRecieverViewModel"
             x:DataType="viewmodels:TransientMsgRecieverViewModel"
       
             Title="CVDemo">
    <viewbase:ViewBase.PageContent>
   <VerticalStackLayout VerticalOptions="Center" HorizontalOptions="Center" Spacing="20">

            <Button
                Text="Load sending page from transient page"
                Command="{Binding LoadMessageSenderCommand}"/>


              <Button
                Text="Back to first page"
                Command="{Binding NavBackCommand}"/>

   </VerticalStackLayout>
        </viewbase:ViewBase.PageContent>
</viewbase:ViewBase>
